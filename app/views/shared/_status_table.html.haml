%table.table.table-striped
  %thead
    %tr
      %th ID
      %th Date
      %th Status
      %th Name
  %tbody
  - table.items.each do |item|
    %tr
      %td
        = link_to(item.id, item.uri)
      %td
        = item.date
      %td.text-center
        = item.status
      %td
        = item.name
= paginate @table_items
